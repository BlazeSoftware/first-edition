{
  "hosting": {
    "public": "www",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/contact/**",
        "function": "contact"
      },
      {
        "source": "/analytics/**",
        "function": "analytics"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
